{
    "AWSEBDockerrunVersion": 1,
    "containerDefinitions": [
      {
        "name": "client",
        "image":{
        "name": "sagardokhale29/multi-client",
        "update": true
        },
        "hostname": "client",
        "essential": false,
        "memory": 128
      },
      {
        "name": "server",
        "image": {
            "name":"sagardokhale29/multi-server",
            "update": true
        },
        "hostname": "api",
        "essential": false,
        "memory": 128
      },
      {
        "name": "worker",
        "image": {
            "name":"sagardokhale29/multi-worker",
             "update": true
        },
        "hostname": "worker",
        "essential": false,
        "memory": 128
      },
      {
        "name": "nginx",
        "image": {
            "name": "sagardokhale29/multi-nginx",
            "update": true
        },
        "hostname": "nginx",
        "essential": true,
        "portMappings": [
          {
            "hostPort": 80,
            "containerPort": 80
          }
        ],
        "links": ["client", "server"],
        "memory": 128
      }
    ]
  }
  